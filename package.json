{
  "name": "typescript-ddd-skeleton",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "url": "https://github.com/CodelyTV/typescript-ddd-skeleton"
  },
  "license": "public",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.14.0"
  },
  "scripts": {
    "dev": "ts-node-dev ./src/App/start.ts",
    "build": "rimraf ./dist && tsc",
    "start": "npm run build && node ./dist/App/start.js",
    "lint": "tslint src/**/*.ts{,x}",
    "test": "npm run test:unit && npm run test:features",
    "test:unit": "NODE_ENV=test jest",
    "test:features": "npm run test:sedes:sedes && npm run test:liquidaciones:postliquidaciones",
    "test:sedes:sedes": "NODE_ENV=test cucumber-js -p sedes",
    "test:liquidaciones:postliquidaciones": "NODE_ENV=test cucumber-js -p liquidaciones",
    "build:tsc": "tsc -p tsconfig.prod.json",
    "build:di": "copy 'src/**/*.{json,yaml,html,png}' dist/src",
    "build:clean": "rm -r dist; exit 0"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "body-parser": "^1.19.0",
    "bodybuilder": "^2.4.0",
    "bson": "^4.5.2",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "copy": "^0.3.2",
    "dotenv": "^16.0.1",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-promise-router": "^4.1.0",
    "express-validator": "^6.12.2",
    "glob": "^7.2.0",
    "helmet": "^4.6.0",
    "http-status": "^1.5.0",
    "moment": "^2.29.4",
    "mongoose": "^6.5.2",
    "node-dependency-injection": "^2.7.1",
    "nunjucks": "^3.2.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/connect-flash": "0.0.37",
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-session": "^2.0.43",
    "@types/cucumber": "^6.0.1",
    "@types/errorhandler": "1.5.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.8",
    "@types/glob": "^7.1.4",
    "@types/helmet": "0.0.48",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.10.2",
    "@types/nunjucks": "^3.2.0",
    "@types/supertest": "^2.0.11",
    "cucumber": "^6.0.5",
    "faker": "^5.5.3",
    "husky": "^7.0.2",
    "jest": "^27.2.4",
    "lint-staged": "11.2.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,tests}/**/*.ts": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  }
}
